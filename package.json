{
  "name": "@lottojs/documentation",
  "version": "0.1.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "author": "Pedro Harbs <harbspj@gmail.com>",
  "keywords": [
    "nodejs",
    "http",
    "router",
    "typescript",
    "lottojs",
    "documentation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lottojs/documentation.git"
  },
  "bugs": {
    "url": "https://github.com/lottojs/documentation/issues"
  },
  "homepage": "https://github.com/lottojs/documentation#readme",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install"
  },
  "devDependencies": {
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "validate-branch-name": "^1.3.0",
    "vitepress": "1.0.0-rc.10"
  },
  "lint-staged": {
    "docs/**/*": "npm run lint"
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feat|fix|imp|release)\/.+$"
  }
}
